<?xml version="1.0" encoding="utf-8"?>
<androidx.appcompat.widget.LinearLayoutCompat xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    android:background="#ffffff"
    >

    <androidx.appcompat.widget.Toolbar
        android:id="@+id/toolbar"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:visibility="visible"
        android:background="#FAFAFA"
        android:layout_gravity="center"
        android:gravity="center"
        app:title="Support"
        app:navigationIcon="@drawable/ic_keyboard_arrow_left_black_24dp"
        app:titleTextColor="@color/black"
        android:theme="@style/ThemeOverlay.AppCompat.Dark"
        app:layout_constraintTop_toTopOf="@+id/fragment_container1"/>


    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="wrap_content">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical">


            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_margin="10dp"
                android:padding="10dp"
                android:orientation="horizontal">

                <ImageView
                    android:layout_width="@dimen/dim_30"
                    android:layout_height="@dimen/dim_30"
                    android:layout_gravity="center"
                    android:layout_marginLeft="10dp"
                    android:src="@drawable/privacy_icon">

                </ImageView>

                <LinearLayout
                    android:layout_width="wrap_content"
                    android:orientation="vertical"
                    android:layout_weight="1"
                    android:layout_marginLeft="30dp"
                    android:layout_height="wrap_content">

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="My Account"
                        android:id="@+id/tfare"
                        android:textColor="#000"
                        android:textSize="18sp"
                        android:layout_marginRight="5dp"
                        android:layout_toLeftOf="@+id/image"
                        >

                    </TextView>
                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="Accounts, Referrals &amp; more"
                        android:layout_marginTop="5dp"
                        android:textSize="15sp"
                        android:layout_marginRight="5dp"
                        android:layout_toLeftOf="@+id/image"
                        >

                    </TextView>




                </LinearLayout>
                <ImageView
                    android:layout_width="@dimen/dim_20"
                    android:layout_height="@dimen/dim_20"
                    android:layout_gravity="right"

                    android:layout_marginLeft="20dp"
                    android:src="@drawable/ic_baseline_chevron_right_24">

                </ImageView>

            </LinearLayout>
            <View
                android:layout_width="match_parent"
                android:layout_height="0.5dp"
                android:layout_marginLeft="80dp"
                android:background="#000"></View>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_margin="10dp"
                android:padding="10dp"
                android:orientation="horizontal">

                <ImageView
                    android:layout_width="@dimen/dim_30"
                    android:layout_height="@dimen/dim_30"
                    android:layout_gravity="center"
                    android:layout_marginLeft="10dp"
                    android:src="@drawable/privacy_icon">

                </ImageView>

                <LinearLayout
                    android:layout_width="wrap_content"
                    android:orientation="vertical"
                    android:layout_weight="1"
                    android:layout_marginLeft="30dp"
                    android:layout_height="wrap_content">

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="Fares &amp; Payments"
                        android:textColor="#000"
                        android:textSize="18sp"
                        android:layout_marginRight="5dp"
                        android:layout_toLeftOf="@+id/image"
                        >

                    </TextView>
                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="TaxiMoh money &amp; other modes"
                        android:layout_marginTop="5dp"
                        android:textSize="15sp"
                        android:layout_marginRight="5dp"
                        android:layout_toLeftOf="@+id/image"
                        >

                    </TextView>




                </LinearLayout>
                <ImageView
                    android:layout_width="@dimen/dim_20"
                    android:layout_height="@dimen/dim_20"
                    android:layout_gravity="right"

                    android:layout_marginLeft="20dp"
                    android:src="@drawable/ic_baseline_chevron_right_24">

                </ImageView>

            </LinearLayout>
            <View
                android:layout_width="match_parent"
                android:layout_height="0.5dp"
                android:layout_marginLeft="80dp"
                android:background="#000"></View>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_margin="10dp"
                android:padding="10dp"
                android:orientation="horizontal">

                <ImageView
                    android:layout_width="@dimen/dim_30"
                    android:layout_height="@dimen/dim_30"
                    android:layout_gravity="center"
                    android:layout_marginLeft="10dp"
                    android:src="@drawable/privacy_icon">

                </ImageView>

                <LinearLayout
                    android:layout_width="wrap_content"
                    android:orientation="vertical"
                    android:layout_weight="1"
                    android:layout_marginLeft="30dp"
                    android:layout_height="wrap_content">

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="Safety"
                        android:textColor="#000"
                        android:textSize="18sp"
                        android:layout_marginRight="5dp"
                        android:layout_toLeftOf="@+id/image"
                        >

                    </TextView>
                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="Safety queries &amp; concerns"
                        android:layout_marginTop="5dp"
                        android:textSize="15sp"
                        android:layout_marginRight="5dp"
                        android:layout_toLeftOf="@+id/image"
                        >

                    </TextView>




                </LinearLayout>
                <ImageView
                    android:layout_width="@dimen/dim_20"
                    android:layout_height="@dimen/dim_20"
                    android:layout_gravity="right"

                    android:layout_marginLeft="20dp"
                    android:src="@drawable/ic_baseline_chevron_right_24">

                </ImageView>

            </LinearLayout>
            <View
                android:layout_width="match_parent"
                android:layout_height="0.5dp"
                android:layout_marginLeft="80dp"
                android:background="#000"></View>


            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_margin="10dp"
                android:padding="10dp"
                android:orientation="horizontal">

                <ImageView
                    android:layout_width="@dimen/dim_30"
                    android:layout_height="@dimen/dim_30"
                    android:layout_gravity="center"
                    android:layout_marginLeft="10dp"
                    android:src="@drawable/privacy_icon">

                </ImageView>

                <LinearLayout
                    android:layout_width="wrap_content"
                    android:orientation="vertical"
                    android:layout_weight="1"
                    android:layout_marginLeft="30dp"
                    android:layout_height="wrap_content">

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="All about TaxiMoh services"
                        android:textColor="#000"
                        android:textSize="18sp"
                        android:layout_marginRight="5dp"
                        android:layout_toLeftOf="@+id/image"
                        >

                    </TextView>
                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="Daily rides , rental , outstation &amp; more"
                        android:layout_marginTop="5dp"
                        android:textSize="15sp"
                        android:layout_marginRight="5dp"
                        android:layout_toLeftOf="@+id/image"
                        >

                    </TextView>




                </LinearLayout>
                <ImageView
                    android:layout_width="@dimen/dim_20"
                    android:layout_height="@dimen/dim_20"
                    android:layout_gravity="right"

                    android:layout_marginLeft="20dp"
                    android:src="@drawable/ic_baseline_chevron_right_24">

                </ImageView>

            </LinearLayout>
            <View
                android:layout_width="match_parent"
                android:layout_height="0.5dp"
                android:layout_marginLeft="80dp"
                android:background="#000"></View>
        </LinearLayout>


    </ScrollView>


</androidx.appcompat.widget.LinearLayoutCompat>

